  <div class="loadBoardDialog modal-dialog modal-sm">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">X</button>
        <h4 class="modal-title">Load a Board</h4>
      </div>
      <div class="modal-body">
        <ul>
            <% for(var i=0; i < boards.length; i++) {%>
          <button id=<%= 'boardname' + boards[i] %> type="button"
                  class="boardName btn userBtn"
                  onClick="fetchBoard(<%= boards[i] %>)"><%= boards[i] %></button>
            <% } %>
        </ul>
      </div>
    </div>
    <button id="loadBoardClose" type="button" class="btn hiddenBtn" data-toggle="modal" data-target="#loadBoardModal"></button>
  </div>
